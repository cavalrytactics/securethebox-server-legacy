{
  "name": "securethebox-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "python git_scripts.py dev",
    "prod": "python git_scripts.py prod",
    "pip-s": "python git_scripts.py pip-save",
    "push-m": "python git_scripts.py git-push-master",
    "push-b": "python git_scripts.py git-push-branch",
    "sync-l": "python git_scripts.py git-sync-local",
    "sync-m": "python git_scripts.py git-merge-upstream",
    "compose-up": "docker-compose up",
    "compose-down": "docker-compose down --remove-orphans",
    "docker-b": "python git_scripts.py docker-build",
    "docker-r": "python git_scripts.py docker-run",
    "docker-k": "python git_scripts.py docker-kill-all",
    "elk": "docker run --rm -p 5601:5601 -p 9200:9200 -p 5044:5044 -it --name elk sebp/elk",
    "rabbit": "docker run -d -p 5672:5672 rabbitmq",
    "postgres": "docker run -d -p 5432:5432 -e POSTGRES_PASSWORD=airflow -e POSTGRES_USER=airflow -e POSTGRES_DB=airflow postgres",
    "celery-app": "./venv/bin/python3.7 celery_app.py",
    "work-start": ". ./venv/bin/activate ; celery -A celery_tasks multi start worker --loglevel=info --pool=eventlet --autoscale=1,4 -O fair",
    "work-stop": ". ./venv/bin/activate ; celery -A celery_tasks multi stop worker",
    "beat-start": "rm -rf beat-worker*.log ; . ./venv/bin/activate ; celery -A celery_beats multi start beat-worker -B --loglevel=info --autoscale=1,4 -O fair",
    "beat-stop": "rm -rf beat-worker*.log ; . ./venv/bin/activate ; celery -A celery_beats multi stop beat-worker",
    "flower": ". ./venv/bin/activate ; flower -A celery_tasks --port=5555",
    "images-a": "python git_scripts.py docker-images-delete-all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ncmd/securethebox-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ncmd/securethebox-server/issues"
  },
  "homepage": "https://github.com/ncmd/securethebox-server#readme",
  "devDependencies": {
    "cz-conventional-changelog": "^3.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "b": "$CURRENTBRANCH"
  }
}
